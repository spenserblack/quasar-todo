{
  "name": "quasar-todo",
  "version": "0.1.3",
  "description": "A to-do list built with Electron + Quasar",
  "productName": "Quasar Todo",
  "author": "Spenser Black",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev-electron": "quasar dev -m electron",
    "build": "quasar build",
    "build-electron": "quasar build -m electron",
    "build-all": "yarn build && yarn build-electron",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format-check": "prettier --check \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.6",
    "electron-updater": "^6.1.1",
    "pinia": "^2.0.35",
    "quasar": "^2.12.5",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "typanion": "^3.12.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.4.7",
    "@quasar/cli": "^2.2.3",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.15",
    "electron": "^22.3.24",
    "electron-builder": "^24.4.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "typescript": "~4.8",
    "vite": "^2.9.16"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
