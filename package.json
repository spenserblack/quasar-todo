{
  "name": "quasar-todo",
  "version": "0.1.2",
  "description": "A to-do list built with Electron + Quasar",
  "productName": "Quasar Todo",
  "author": "Spenser Black",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev-electron": "quasar dev -m electron",
    "build": "quasar build",
    "build-electron": "quasar build -m electron",
    "build-all": "yarn build && yarn build-electron",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format-check": "prettier --check \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "electron-updater": "^5.3.0",
    "pinia": "^2.0.11",
    "quasar": "^2.11.7",
    "sequelize": "^6.29.0",
    "sqlite3": "^5.1.4",
    "typanion": "^3.12.1",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.2.1",
    "@quasar/cli": "^1.3.2",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.13",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.1.0",
    "prettier": "^2.8.4",
    "typescript": "~4.8",
    "vite": "^2.0.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
